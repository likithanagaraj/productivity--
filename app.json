{
  "expo": {
    "name": "learn-expo",
    "slug": "learn-expo",
    "version": "1.0.0",
    "scheme": "learn-expo",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181B"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#18181B"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
      
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
